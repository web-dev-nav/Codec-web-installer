{
    "name": "codelone/codec-web-installer",
    "description": "Codec Web Installer - WordPress-like installer package for Laravel applications",
    "version": "1.0.1",
    "type": "library",
    "keywords": ["laravel", "installer", "setup", "deployment"],
    "license": "MIT",
    "authors": [
        {
            "name": "Navjot",
            "email": "support@codelone.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codelone\\CodecWebInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codelone\\CodecWebInstaller\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codelone\\CodecWebInstaller\\InstallerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}